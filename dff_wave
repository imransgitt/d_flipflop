S_0x562d7a013840 .scope module, "dff_tb" "dff_tb" 2 1;
 .timescale 0 0;
v0x562d7a039920_0 .var "clk", 0 0;
v0x562d7a0399e0_0 .var "d", 0 0;
v0x562d7a039ab0_0 .net "q", 0 0, v0x562d7a0396e0_0;  1 drivers
v0x562d7a039bb0_0 .var "reset", 0 0;
S_0x562d7a027b20 .scope module, "dut" "d_ff" 2 5, 3 1 0, S_0x562d7a013840;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "d";
    .port_info 3 /OUTPUT 1 "q";
v0x562d79fef7b0_0 .net "clk", 0 0, v0x562d7a039920_0;  1 drivers
v0x562d79fefba0_0 .net "d", 0 0, v0x562d7a0399e0_0;  1 drivers
v0x562d7a0396e0_0 .var "q", 0 0;
v0x562d7a0397b0_0 .net "reset", 0 0, v0x562d7a039bb0_0;  1 drivers
E_0x562d79fee700 .event posedge, v0x562d79fef7b0_0;
    .scope S_0x562d7a027b20;
T_0 ;
    %wait E_0x562d79fee700;
    %load/vec4 v0x562d7a0397b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x562d7a0396e0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x562d79fefba0_0;
    %assign/vec4 v0x562d7a0396e0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x562d7a013840;
T_1 ;
    %vpi_call 2 8 "$dumpfile", "test_dff.vcd" {0 0 0};
    %vpi_call 2 9 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x562d7a013840 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x562d7a013840;
T_2 ;
    %delay 2, 0;
    %load/vec4 v0x562d7a039920_0;
    %inv;
    %store/vec4 v0x562d7a039920_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x562d7a013840;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562d7a039920_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562d7a039bb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562d7a0399e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562d7a039bb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x562d7a0399e0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x562d7a0399e0_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 26 "$finish" {0 0 0};
    %end;
